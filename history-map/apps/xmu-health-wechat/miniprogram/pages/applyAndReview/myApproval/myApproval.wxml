                            <!-- 拒绝/同意 -->
                            <view  class="weui-form-preview__ft" style="background-color:=#F8F8F8">
                                <view wx:if="{{MyAppr.state==1}}"class="weui-form-preview__btn weui-form-preview__btn_default"  style="color:#228B22" hover-class="weui-form-preview__btn_active">已通过</view>
                                <view wx:if="{{MyAppr.state==-1}}"class="weui-form-preview__btn weui-form-preview__btn_default"  style="color:#B22222" hover-class="weui-form-preview__btn_active">已拒绝</view>
                            </view> 
                          </view>     
                        </view>
                      </view>
                      <!-- 底部提示 -->
                      <view class="weui-form-preview__ft "style="height:120rpx;margin-top: 15rpx;">
                          <view class="weui-form-preview__btn weui-form-preview__btn_default" hover-class="weui-form-preview__btn_active">——</view>
                      </view>

                  </view>
        </view>
    </view>
</view>


