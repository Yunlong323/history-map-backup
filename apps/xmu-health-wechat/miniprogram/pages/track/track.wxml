<view wx:if="{{items!=''}}" class='trackListView-have'>
 <block wx:for="{{items}}" wx:key="key">
  <view class='item'>

   <view wx:if="{{item.type==1}}" class='typeView-green'>
   </view>

   <view wx:if="{{item.type==2}}" class='typeView-yello'>
   </view>

   <view class='placeNameView'>
    <text class='placeNameText'>{{item.place}}</text>
   </view>

   <view class='timeView'>
    <text class = 'timeText'>{{item.time}}</text>
   </view>

  </view>
 </block>
</view>


<view wx:else class='trackListView-none'>
  <view class="weui-loadmore weui-loadmore_line">
    <view class="weui-loadmore__tips weui-loadmore__tips_in-line">暂无数据</view>
  </view>
</view>